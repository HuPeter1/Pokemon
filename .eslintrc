{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/strict"
    // "plugin:@typescript-eslint/stylistic"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "root": true,
  "parserOptions": {
    "ecmaVersion": 12,
    "project": ["tsconfig.json"]
  },
  "rules": {
    "comma-dangle": 0,
    "no-trailing-spaces": "error",
    "no-unused-vars": "warn"
  }
}